<div class="row g-4 mb-4" id="listeOuvrier">
    <div class="col-12 col-md-12">
        <div class="app-card app-card-settings shadow-sm p-4">
            <div class="app-card-body">
                <div class="table-responsive">
                    <table class="table mb-0 text-left" id="ouvrierListTab">
                        <thead>
                            <tr>
                                <th class="h-cell">Ouvrier</th>
                                <th class="h-cell">Categorie</th>
                                <th class="h-cell">Statut</th>
                                <th class="h-cell">TÃ©lephone</th>
                                <th class="h-cell">Urgence</th>
                                <th class="h-cell"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% 
                            for(var i=0; i< ouvriers.length; i++) {
                                let ouvrier = ouvriers[i];
                            %>
                                <tr>
                                    <td class="cell">
                                        <%= ouvrier.nom+" "+ouvrier.prenom %>
                                    </td>
                                    <td class="cell">
                                        <%= ouvrier.categorie.intitule %>
                                    </td>
                                    <td class="cell">
                                        <%= ouvrier.status.intitule %>
                                    </td>
                                    <td class="cell">
                                        <%= ouvrier.contact %>
                                    </td>
                                    <td class="cell">
                                        <%= ouvrier.urgence %>
                                    </td>
                                    <td class="cell">
                                        <a class="btn-sm app-btn-secondary" href="/ouvrier/details/<%= ouvrier.id %>">Voir</a>
                                    </td>
                                </tr>
                                <%
                            }
                            %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>